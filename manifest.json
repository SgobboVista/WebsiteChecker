{
  "manifest_version": 3,
  "name": "SGOVI Engine v15.9",
  "version": "15.9",
  "permissions": ["storage", "activeTab"],
  "host_permissions": [
    "https://sgobbovista.de/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.google.com/search*", "https://www.google.de/search*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}